			Connection con = DriverManager.getConnection
					("jdbc:oracle:thin:@localhost:1521:xe","system","tonystark");
			
			Statement st = con.createStatement();
			
			ResultSet rs = st.executeUpdate("");
			
			
			
			
		}catch (Exception e) 
		{
			e.printStackTrace();
		}
		

	}

}
